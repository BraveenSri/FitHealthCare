<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.braveen.fit_health_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Fit Health Care"
        android:roundIcon="@mipmap/round_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignInPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpPage" />
        <activity android:name=".BioPage" />
        <activity android:name=".GoToPage" />
        <activity android:name=".LeaderboardPage" />
        <activity android:name=".DailyReportPage" />
        <activity android:name=".SetTargetPage" />
        <activity android:name=".WorkoutPage"></activity>
    </application>

</manifest>